rt {\n  ChatContainer,\n  StyledContainer,\n  ChatBox,\n  StyledButton,\n  SendIcon\n} from './styles';\nimport  List  from '../../components/List';\nimport { Input } from 'antd';\nfunction ChatRoom(props) {\n    const {username, room, joinData } = props;\n    const [messages, setMessages] = useState([]);\n    const [message, setMessage] = useState([]);\n    const [users, setUsers] = useState([]);\n    useEffect(() => {\n        if( Object.keys(joinData).length > 0) {\n            setMessages([joinData])    \n            socket.on('message', (message, error) => {\n                setMessages(msgs => [ ...msgs, message ]);\n            });\n            socket.on(\"roomInfo\", (users) => {\n                setUsers(users);\n            });\n        } \n        else {\n            history.push('/join')\n        }\n     }, [joinData])\n     console.log(users)\n     function sendOneMessage  (message) {\n        \n           if(message) {\n               console.log(message)\n               socket.emit('sendMessage',{ userId: joinData.userData.id, message }, (error) => {\n                   if(error) {\n                       alert(error)\n                       history.push('/join');\n                   }\n               });\n               setMessage('')\n           } else {\n               alert(\"Message can't be empty\")\n           }\n   }\n     const handleChange = (e) => {\n        setMessage(e.target.value);\n      }\n      \n      const handleClick = (e) => {\n        console.log(message)\n        sendOneMessage(message);\n    \n      }\n      \n       \n\n       return (\n        <ChatContainer>\n            <Header room={room} />\n            <StyledContainer>\n                <  List users={users.users}/>\n                <ChatBox>\n                    <Messages messages={messages} username={username}/>\n                    <Input type=\"text\" placeholder=\"Type your message\" value={message} onChange={handleChange}/>\n                            <StyledButt