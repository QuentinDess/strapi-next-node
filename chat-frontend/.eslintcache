ledButton>\n        </StyledCard>\n    )\n};\n\nexport default JoinRoom;\n\nconst StyledCard = styled(Card)`\n    width: 581px;\n    height: 210px;\n    margin: 30vh auto;\n    box-shadow: 2px 3px 3px 2.8px #d7d7e4;\n    text-align: center;\n`\nconst StyledButton = styled(Button)`\n    margin-top: 10px;\n`",["126","127"],"/Users/admin/blog/strapi-blog-ui-starter/src/screens/JoinRoom/index.js",["128"],"/Users/admin/blog/chat-frontend/src/index.js",[],"/Users/admin/blog/chat-frontend/src/reportWebVitals.js",[],"/Users/admin/blog/chat-frontend/src/App.js",[],"/Users/admin/blog/chat-frontend/src/config/network.js",[],"/Users/admin/blog/chat-frontend/src/screens/JoinRoom/index.js",["129"],"/Users/admin/blog/chat-frontend/src/screens/ChatRoom/index.js",["130"],"/Users/admin/blog/chat-frontend/src/config/web-sockets.js",[],"/Users/admin/blog/chat-frontend/src/screens/ChatRoom/styles.js",[],"/Users/admin/blog/chat-frontend/src/components/Header/index.js",[],"/Users/admin/blog/chat-frontend/src/components/List/index.js",[],"/Users/admin/blog/chat-frontend/src/components/Messages/index.js",[],"/Users/admin/blog/chat-frontend/src/components/Messages/Message/index.js",[],"/Users/admin/blog/chat-frontend/src/components/Messages/Message/styles.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":2,"column":8,"nodeType":"137","messageId":"138","endLine":2,"endColumn":14},{"ruleId":"135","severity":1,"message":"139","line":2,"column":8,"nodeType":"137","messageId":"138","endLine":2,"endColumn":22},{"ruleId":"135","severity":1,"message":"140","line":3,"column":8,"nodeType":"137","messageId":"138","endLine":3,"endColumn":15},{"ruleId":"135","severity":1,"message":"136","line":4,"column":8,"nodeType":"137","messageId":"138","endLine":4,"endColumn":14},{"ruleId":"131","replacedBy":"141"},{"ruleId":"133","replacedBy":"142"},{"ruleId":"135","severity":1,"message":"143","line":8,"column":12,"nodeType":"137","mes